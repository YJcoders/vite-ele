    "axios": "^1.1.3",
    "crypto-es": "^1.2.7",
    "dayjs": "^1.11.6",
    "echarts": "^5.4.1",
    "element-plus": "^2.2.18",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "pinia": "^2.0.21",
    "pinia-plugin-persistedstate": "^2.3.0",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
