{
  "name": "vue-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only --mode=production",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
