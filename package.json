    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "pina": "^0.20.2204228",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "typescript": "~4.7.4",
    "vite": "^4.2.0",
    "vitest": "^0.23.0",
    "vue-tsc": "^0.40.7"
  }
